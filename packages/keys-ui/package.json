{
  "name": "@keys/ui",
  "version": "1.2.9",
  "description": "Modern Blade components library for Laravel",
  "type": "module",
  "main": "dist/keys-ui.umd.js",
  "module": "dist/keys-ui.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "resources",
    "src",
    "config"
  ],
  "exports": {
    "./js": "./dist/keys-ui.umd.js",
    ".": {
      "import": "./dist/keys-ui.es.js",
      "require": "./dist/keys-ui.umd.js"
    }
  },
  "scripts": {
    "dev": "vite --port 5174",
    "build": "npm run clean && vite build && npm run build:copy",
    "build:copy": "cp dist/keys-ui.umd.js dist/keys-ui.min.js",
    "build:verify": "node scripts/verify-build.js",
    "watch": "vite build --watch",
    "preview": "vite preview --port 5174",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "ci": "npm run typecheck && npm run build && npm run build:verify",
    "sync-version": "node scripts/sync-version.js"
  },
  "dependencies": {
    "@oddbird/css-anchor-positioning": "^0.1.0",
    "quill": "^2.0.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^20.0.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.0",
    "postcss": "^8.4.0",
    "postcss-cli": "^11.0.0",
    "rimraf": "^5.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "tailwindcss": "^4.0.0"
  },
  "keywords": [
    "laravel",
    "blade",
    "components",
    "ui",
    "tailwind",
    "keys-ui"
  ],
  "author": "Keys UI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keys-ui/keys-ui.git"
  },
  "bugs": {
    "url": "https://github.com/keys-ui/keys-ui/issues"
  },
  "homepage": "https://github.com/keys-ui/keys-ui#readme"
}
